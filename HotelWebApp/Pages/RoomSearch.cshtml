@page
@model HotelWebApp.Pages.RoomSearchModel
@{
    ViewData["Title"] = "RoomSearch";
}
<h1>RoomSearch</h1>
<form method="post" class="form-control">
    <div class="form-group">
        <label asp-for="@Model.StartDate"></label>
        <input type="date" asp-for="@Model.StartDate" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="@Model.EndDate"></label>
        <input asp-for="@Model.EndDate" type="date" class="form-control" />
    </div>
    <button type="submit" class="btn btn-primary mt-3">Search</button>
    </form>
